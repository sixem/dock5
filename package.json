{
  "name": "dock5",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=20.19.0 <21 || >=22.12.0"
  },
  "scripts": {
    "dev": "tsx scripts/site/dev.ts",
    "build": "tsx scripts/site/build.ts",
    "preview": "pnpm exec vite preview",
    "generate": "tsx scripts/site/generate.ts",
    "pretest": "pnpm generate",
    "test": "pnpm exec vitest run",
    "test:watch": "pnpm exec vitest",
    "lint": "pnpm exec biome check .",
    "format": "pnpm exec biome format --write .",
    "pretypecheck": "pnpm generate",
    "typecheck": "pnpm exec tsc --noEmit"
  },
  "dependencies": {
    "preact": "^10.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@preact/preset-vite": "^2.0.0",
    "@types/node": "^20.0.0",
    "happy-dom": "^20.5.0",
    "tsx": "^4.8.1",
    "typescript": "^5.0.0",
    "vite": "^7.0.0",
    "vitest": "^2.0.0"
  }
}
